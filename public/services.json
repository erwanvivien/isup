[
  {
    "name": "Xiaojiba.dev",
    "kind": "website",
    "url": "https://xiaojiba.dev",
    "interval": 60,
    "commands": [
      {
        "name": "ping",
        "command": "ping -c 1 -W 10 xiaojiba.dev",
        "retcode": 0,
        "stdout": "1 packets transmitted, 1 received, 0% packet loss, time 0ms"
      },
      {
        "name": "curl",
        "command": "curl --head https://xiaojiba.dev 2>/dev/null",
        "retcode": 0,
        "stdout": "HTTP/2 200"
      },
      {
        "name": "certificates",
        "command": "./public/certificates.sh 'xiaojiba.dev' '80'",
        "retcode": 0
      }
    ]
  }
]
